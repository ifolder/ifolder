<configuration>
  <section name="ServiceManager">
	<setting name="WebServicePath" value="@_webdir_@" />
    <setting name="Services">
      <Service name="Simias Enterprise Location Provider" assembly="Simias" enabled="True" type="Thread" class="Simias.Location.EnterpriseLocation" />
      <Service name="Simias Domain Watcher" assembly="Simias" enabled="True" type="Thread" class="Simias.DomainWatcher.Service" />
      <Service name="Simias Change Log Service" assembly="Simias" enabled="True" type="Thread" class="Simias.Storage.ChangeLog" />
      <Service name="Simias Interprocess Event Service" assembly="Simias" enabled="True" type="Thread" class="Simias.Event.IProcEventServer" />
      <Service name="Simias PO Service" assembly="Simias" enabled="True" type="Thread" class="Simias.POBox.POService" />
      <Service name="Simias Sync Client" assembly="Simias" enabled="True" type="Thread" class="Simias.Sync.SyncClient" />
      <Service name="Rendezvous Workgroup Domain" assembly="Simias.mDns.dll" enabled="False" type="Thread" class="Simias.mDns.Service" />
      <Service name="Gaim Domain" assembly="Simias.Gaim.dll" enabled="False" type="Thread" class="Simias.Gaim.GaimService" />
    </setting>
  </section>
  <section name="StoreProvider">
    <setting name="Assembly" value="@_StoreProviderAssembly_@" />
    <setting name="Type" value="@_StoreProviderType_@" />
  </section>
</configuration>
