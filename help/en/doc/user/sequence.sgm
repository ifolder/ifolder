<!DOCTYPE TOPIC [

<!ELEMENT TOPIC - - (FILENAME, TITLE, TOPIC*)>
<!ATTLIST TOPIC 
	TYPE (ExpandableHeirarchy|NonExpandableHeirarchy|File|InvisibleFile|InvisibleChapter) #REQUIRED
	ELEMTYPE CDATA #REQUIRED
	PREFACE CDATA #IMPLIED
>

<!ELEMENT FILENAME - - (#PCDATA)>
<!ELEMENT TITLE - - (B|SUP|#PCDATA)+>
<!ELEMENT SUP - - (SMALL|#PCDATA)>
<!ELEMENT SMALL - - (#PCDATA)>
<!ELEMENT B - - (#PCDATA)>

<!ENTITY lt     SDATA "[lt    ]"--=less-than sign R:-->
<!ENTITY equals SDATA "[equals]"--=equals sign R:-->
<!ENTITY gt     SDATA "[gt    ]"--=greater-than sign R:-->
<!ENTITY amp    SDATA "[amp   ]"--=ampersand-->
<!ENTITY bsol   SDATA "[bsol  ]"--/backslash =reverse solidus-->
<!ENTITY copy   SDATA "[copy  ]"--=copyright sign-->
<!ENTITY reg    SDATA "[reg   ]"--/circledR =registered sign-->
<!ENTITY trade  SDATA "[trade ]"--=trade mark sign-->
<!ENTITY quot   SDATA "[quot  ]"--=quotation mark-->
<!ENTITY apos   SDATA "[apos  ]"--=apostrophe-->
<!ENTITY lsquo  SDATA "[lsquo ]"--=single quotation mark, left-->
<!ENTITY rsquo  SDATA "[rsquo ]"--=single quotation mark, right-->
<!ENTITY ldquo  SDATA "[ldquo ]"--=double quotation mark, left-->
<!ENTITY rdquo  SDATA "[rdquo ]"--=double quotation mark, right-->
<!ENTITY nbsp   SDATA "[nbsp  ]"--=no break (required) space-->

<!--
2 = ExpandableHeirarchy - This is equivalent to a book, chapter, appendix etc that has viewable children
4 = NonExpandableHeirarchy	-  Same as ExpandableHeirarchy except children are not viewable
6 = File - Is itself viewable and has no children
8 = InvisibleFile -  is not viewable and has no chilren
C = InvisibleChapter	-  Used by the HLP2MOAB process only as a way to group a bunch of files that should not show up in the toc
-->
